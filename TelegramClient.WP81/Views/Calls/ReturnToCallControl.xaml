<UserControl x:Class="TelegramClient.Views.Calls.ReturnToCallControl"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    FontFamily="{StaticResource PhoneFontFamilyNormal}"
    FontSize="{StaticResource PhoneFontSizeNormal}"
    Foreground="{StaticResource PhoneForegroundBrush}"
    Height="68"
    d:DesignHeight="480" d:DesignWidth="480">
    
    <Grid x:Name="LayoutRoot" Background="{StaticResource TelegramBadgeAccentBrush}">
        <TextBlock Text="{Binding Resources.TouchToReturnToCall, Source={StaticResource Strings}}" Foreground="White" Margin="12" HorizontalAlignment="Center" TextWrapping="Wrap" VerticalAlignment="Bottom"/>
    </Grid>
</UserControl>